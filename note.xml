<device-manager-config>

<!-- Enable option if using CTM. -->
<CTM-enabled>Y</CTM-enabled>

<!-- URL of CTM Service to connect when initializing CTM. -->
<CTM-URL>ctm://localhost:3636</CTM-URL>

<!-- Client ID use to connect when initializing CTM. -->
<CTM-Client-ID>localhost</CTM-Client-ID

</device-manager-config>
